{
    "name": "anastalal/laravel-translation-importer",
    "description": "A Laravel Package to synchronise translation from your blade files",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Anastalal\\LaravelTranslationImporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anas Talal",
            "email": "anasctalal@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anastalal\\LaravelTranslationImporter\\Providers\\TranslationImporterServiceProvider",
                "Anastalal\\LaravelTranslationImporter\\Providers\\TranslationImporterBindingsServiceProvider"
            ]
    }
    }
}
